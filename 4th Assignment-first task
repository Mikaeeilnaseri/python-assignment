def nm(n, m):
    for rows in range(0, n):
        if m % 2 == 0:
            if rows % 2 == 0:
                print(m // 2 * '#*')
            else:
                print(m // 2 * '*#')
        else:
            if rows % 2 == 0:
                print(m // 2 * '#*' + '#')
            else:
                print(m // 2 * '*#' + '*')


n = int(input('number of rows n= '))
m = int(input('number of columns m= '))

nm(n, m)
